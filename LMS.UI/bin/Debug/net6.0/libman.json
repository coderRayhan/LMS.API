{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "sweetalert@2.1.2",
      "destination": "wwwroot/lib/sweetalert/"
    },
    {
      "library": "datatable@2.0.2",
      "destination": "wwwroot/lib/datatable/",
      "files": [
        "js/datatable.min.js",
        "js/datatable.js",
        "css/datatable.min.css",
        "css/datatable.css"
      ]
    },
    {
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/font-awesome.min.css",
        "css/font-awesome.css"
      ]
    }
  ]
}